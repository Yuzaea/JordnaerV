@page
@model Jordnaer.Pages.Members.LoginModel
@{
}






<h3>Login form</h3>
@Model.Message
<a class="btn btn-primary" asp-page="/Members/Create">Create a new user</a>
<form method="post">
    <table>
        <tr>
            <td>Email</td>
            <td><input type="text" asp-for="@Model.Email" /></td>
        </tr>
        <tr>
            <td>Password</td>
            <td><input type="password" asp-for="@Model.PassWord" /></td>
        </tr>
        <tr>
            <td>&nbsp;</td>
            <td><input type="submit" value="Login" /></td>
        </tr>
    </table>
</form>



